  <% @team_number.times do %>
    <div class="row"> 
        <div class="col-md-4">
            <img src="<%= Faker::Avatar.image(rand(1..1000), "300x300", "png", "set#{rand(1..4)}", "bg#{rand(1..2)}" )%>"></img>
        </div>
        <div class="col-md-4">
            <h3>
             Superhero Name : 
             <br>
             <span class="red"><%= Faker::Superhero.prefix %> <%=Faker::Superhero.name%></span>
            </h3>
            <h4>
             Superhero Power :<br>
             <span class="red"><%= Faker::Superhero.power %></span><br><br>
            </h4>
             <h4>
             Your undercover Day Job is:<br>
            <span class="corp_red"> <%= Faker::Company.profession %></span><br>
             </h4>
             <h4>
             Your Corporate Superpower is:<br>
             <span class="corp_red"><%= Faker::Company.bs.titleize %></span>
             </h4>
        </div>
        <div class="col-md-4">
            ...
        </div>
    </div> 
    <div class="col-md-12"><hr></div>
<% end %>