<div class="panel panel-default">
  <div class="panel-heading">
   <h3 class="panel-title gray"><%= link_to 'Click Here To Generate A New Superhero', root_path %></h3> 
  </div>
  <div class="panel-body">
    <div class="row"> 
        <div class="col-md-4">
            <img src="<%=@avatar %>"></img>
        </div>
        <div class="col-md-4">
            <h3>
             Superhero Name : 
             <br>
             <span class="red"><%= @hero_prefix %> <%= @hero_name %></span>
            </h3>
            <h4>
             Superhero Power :<br>
             <span class="red"><%= @hero_power %></span><br><br>
            </h4>
             <h4>
             Your undercover Day Job is:<br>
            <span class="corp_red"> <%= @corp_title %></span><br>
             </h4>
             <h4>
             Your Corporate Superpower is:<br>
             <span class="corp_red"><%= @corp_power %></span>
             </h4>
        </div>
        <div class="col-md-4">
            ...
        </div>
    </div> 
  </div>
  <div class="panel-footer"><%= link_to 'Create A Whole Superhero Team', home_team_path %></div>
</div>


